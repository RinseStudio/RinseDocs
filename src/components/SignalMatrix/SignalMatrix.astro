const getClassByValue = (value) => {
  switch (value) {
    case 0:
      return 'bg-slate-100';
    case 1:
      return 'bg-slate-200';
    case 2:
      return 'bg-slate-300';
    case 3:
      return 'bg-slate-400';
    case 4:
      return 'bg-slate-500';
    case 5:
      return 'bg-slate-600';
    default:

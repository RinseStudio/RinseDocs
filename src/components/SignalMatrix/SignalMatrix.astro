<div class="flex items-start gap-4">
  <ul class="grid grid-cols-1 gap-1">
    {
      items.map((item) => (
        <li class="flex flex-none items-center justify-end pl-4">
          <div class="flex h-[15px] items-center gap-1.5 text-[10px] font-semibold text-slate-900">
            <i class={`fa-solid ${item.icon}`} />
            {item.name}
            <Tooltip className="">
              <TooltipTrigger>
                <i class="fa-regular fa-fw fa-question-circle text-slate-400" />
              </TooltipTrigger>
              <TooltipContent className="w-[288px]">
                Triggered by comparing the user's current location with their
                last known location and then detecting trips that are highly
                unlikely, or even impossible.
              </TooltipContent>
            </Tooltip>
          </div>
          {/* <p>{item.description}</p> */}
        </li>
      ))
    }
  </ul>
  <ul class="grid flex-1 grid-cols-1 gap-1">
    {
      items.map((item) => (
        <li class="flex items-center rounded-sm ring-slate-300 ring-offset-4 hover:relative hover:z-10 hover:ring-2">
          <ul class="flex flex-1 gap-1">
            {item.data.map((dataItem) => (
              <li class={`relative h-[15px] w-full hover:z-10`}>
                <Tooltip>
                  <TooltipTrigger>
                    <div
                      x-on:click="quickview_signalHeatMap = true"
                      class={`h-[15px] w-full cursor-pointer rounded-sm ring-offset-2 hover:ring ${getClassByValue(dataItem)}`}
                    />
                  </TooltipTrigger>
                  <TooltipContent className="p-0 bg-slate-900 text-white">
                    <div class="border-b border-slate-700 px-2 py-2 text-[10px] font-semibold uppercase text-slate-300">
                      Jan 29 2024
                    </div>
                    <div class="space-y-2 px-3 py-2.5">
                      <div class="flex flex-none items-center gap-2 text-xs">
                        <div class="flex items-center gap-1">
                          <i
                            class={`fa-solid fa-fw fa-code-commit text-[10px] text-blue-400`}
                          />
                          <div class="w-10 font-medium text-slate-300">
                            Events
                          </div>
                        </div>
                        <div class=" text-slate-400">21,524</div>
                      </div>
                      <div class="flex flex-none items-center gap-2 text-xs">
                        <div class="flex items-center gap-1">
                          <i
                            class={`fa-solid fa-fw fa-user text-[10px] text-blue-400`}
                          />
                          <div class="w-10 font-medium text-slate-300">
                            Users
                          </div>
                        </div>
                        <div class=" text-slate-400">1,152</div>
                      </div>
                    </div>
                  </TooltipContent>
                </Tooltip>

---
import Layout from '@/layouts/rinsedocs/Raw.astro';
import Card from '@rinseui/elements/Card';
---

<Layout layoutClass="">
  <div class="flex items-center justify-center px-7 py-[100px]">
    <Card className="text-center">
      <Card.Group className="px-10 py-10 text-center">
        <i class="fa-solid fa-timer mb-2 text-[40px] text-slate-300"></i>
        <h1 class="mb-2 text-lg font-bold text-black">Your trial has expired</h1>
        <p class="text-sm text-gray-600">Please update your your <a href="#" class="font-medium text-blue-500">billing information</a> to activate your account.</p>
      </Card.Group>
    </Card>
  </div>
</Layout>
